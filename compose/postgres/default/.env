# ── Proxy selection ─────────────────────────────────────────────────────────
TRAEFIK_ENABLE=false
CADDY_ENABLE=false

# ── Database credentials ─────────────────────────────────────────────────────
POSTGRES_USER=postgres
POSTGRES_DB=postgres
# Secret — generated and stored in kpv on first use
POSTGRES_PASSWORD="$(kpv --vault homelab secrets ensure --key default/postgres-password --size 32 -S)"
