# ── Proxy selection ─────────────────────────────────────────────────────────
TRAEFIK_ENABLE=false
CADDY_ENABLE=false

RUSTFS_HOST=rustfs.local
RUSTFS_CONSOLE_HOST=rustfs-console.local
RUSTFS_PORT=9000
RUSTFS_CONSOLE_PORT=9001
# Secrets — generated and stored in kpv on first use
RUSTFS_ACCESS_KEY="$(kpv --vault homelab secrets ensure --key default/rustfs-access-key --size 20 -S)"
RUSTFS_SECRET_KEY="$(kpv --vault homelab secrets ensure --key default/rustfs-secret-key --size 40 -S)"
