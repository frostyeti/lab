# ── Proxy selection ─────────────────────────────────────────────────────────
TRAEFIK_ENABLE=false
CADDY_ENABLE=false

FILERUN_HOST=files.local
FILERUN_PORT=8082
FILERUN_DB_NAME=filerun
FILERUN_DB_USER=filerun
# Secrets — generated and stored in kpv on first use
FILERUN_DB_PASSWORD="$(kpv --vault homelab secrets ensure --key default/filerun-db-password --size 32 -S)"
FILERUN_DB_ROOT_PASSWORD="$(kpv --vault homelab secrets ensure --key default/filerun-db-root-password --size 32 -S)"
