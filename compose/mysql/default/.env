# ── Proxy selection ─────────────────────────────────────────────────────────
TRAEFIK_ENABLE=false
CADDY_ENABLE=false

# ── MySQL ─────────────────────────────────────────────────────────────────────
MYSQL_DATABASE=mydb
MYSQL_USER=dbuser
# Secrets — generated and stored in kpv on first use
MYSQL_ROOT_PASSWORD="$(kpv --vault homelab secrets ensure --key default/mysql-root-password --size 32 -S)"
MYSQL_PASSWORD="$(kpv --vault homelab secrets ensure --key default/mysql-user-password --size 32 -S)"
