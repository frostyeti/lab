# ── Proxy selection ─────────────────────────────────────────────────────────
TRAEFIK_ENABLE=false
CADDY_ENABLE=false

VAULTWARDEN_HOST=vw.local
VAULTWARDEN_PORT=8888
VAULTWARDEN_SIGNUPS=false
# Admin panel token — generate once with: kpv --vault homelab secrets set --key default/vaultwarden-admin-token --generate --size 64
VAULTWARDEN_ADMIN_TOKEN="$(kpv --vault homelab secrets ensure --key default/vaultwarden-admin-token --size 64 -S)"
