# ── Proxy selection ─────────────────────────────────────────────────────────
TRAEFIK_ENABLE=false
CADDY_ENABLE=false

N8N_HOST=n8n.local
N8N_PORT=5678
N8N_PROTOCOL=http
N8N_DB_HOST=postgres
N8N_DB_NAME=n8n
N8N_DB_USER=n8n
# Secrets — generated and stored in kpv on first use
N8N_DB_PASSWORD="$(kpv --vault homelab secrets ensure --key default/n8n-db-password --size 32 -S)"
N8N_ENCRYPTION_KEY="$(kpv --vault homelab secrets ensure --key default/n8n-encryption-key --size 32 -S)"
