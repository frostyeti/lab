# ── Proxy selection ─────────────────────────────────────────────────────────
TRAEFIK_ENABLE=false
CADDY_ENABLE=false

CODE_SERVER_HOST=code.local
CODE_SERVER_PORT=8443
PUID=1000
PGID=1000
TZ=UTC
# Secrets — generated and stored in kpv on first use
CODE_SERVER_PASSWORD="$(kpv --vault homelab secrets ensure --key default/code-server-password --size 32 -S)"
CODE_SERVER_SUDO_PASSWORD="$(kpv --vault homelab secrets ensure --key default/code-server-sudo-password --size 32 -S)"
