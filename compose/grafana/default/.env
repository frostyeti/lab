# ── Proxy selection ─────────────────────────────────────────────────────────
TRAEFIK_ENABLE=false
CADDY_ENABLE=false

GRAFANA_HOST=grafana.local
GRAFANA_PORT=3000
GF_SERVER_ROOT_URL=http://grafana.local
GF_SECURITY_ADMIN_USER=admin
GF_INSTALL_PLUGINS=
# Secret — generated and stored in kpv on first use
GF_SECURITY_ADMIN_PASSWORD="$(kpv --vault homelab secrets ensure --key default/grafana-admin-password --size 32 -S)"
