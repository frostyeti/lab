# ── Proxy selection ─────────────────────────────────────────────────────────
TRAEFIK_ENABLE=false
CADDY_ENABLE=false

POSTHOG_HOST=posthog.local
POSTHOG_PORT=8000
POSTHOG_SITE_URL=http://posthog.local
POSTHOG_DB_NAME=posthog
POSTHOG_DB_USER=posthog
# Secrets — generated and stored in kpv on first use
POSTHOG_DB_PASSWORD="$(kpv --vault homelab secrets ensure --key default/posthog-db-password --size 32 -S)"
POSTHOG_SECRET_KEY="$(kpv --vault homelab secrets ensure --key default/posthog-secret-key --size 50 -S)"
